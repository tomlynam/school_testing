<%= form_for @highschool do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :enrollment %>
  <%= f.text_field :enrollment %>

  <br>

  <%= f.submit nil, id: 'submit-highschool-btn', class: 'btn blue' %>

<% end %>

<br>

<%= link_to 'Back', :back, id: 'cancel-highschool' %>